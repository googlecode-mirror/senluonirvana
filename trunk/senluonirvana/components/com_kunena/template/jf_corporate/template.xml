<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install>
<kinstall type="kunena-template" version="2.0">
	<name>JF Corporate</name>
	<creationDate>27.07.2012</creationDate>
	<version>1.1</version>
	<author>JoomForest</author>
	<authorEmail>info@joomforest.com</authorEmail>
	<authorUrl>http://www.joomforest.com</authorUrl>
	<copyright>JoomForest</copyright>
	<license>GNU/GPL</license>
	<description>JF Corporate theme for Kunena component</description>
	<thumbnail>template_thumbnail.png</thumbnail>
	<params>
		<param name="avatarPosition" type="list" default="left" label="COM_KUNENA_TEMPLATE_PROFILE_POSITION" description="COM_KUNENA_TEMPLATE_PROFILEPOSITION_DESC">
			<option value="left">COM_KUNENA_TEMPLATE_LEFT</option>
			<option value="top">COM_KUNENA_TEMPLATE_TOP</option>
			<option value="right">COM_KUNENA_TEMPLATE_RIGHT</option>
			<option value="bottom">COM_KUNENA_TEMPLATE_BOTTOM</option>
		</param>
		<param name="buttonIconset" type="list" default="default" label="COM_KUNENA_TEMPLATE_BUTTON_SET" description="COM_KUNENA_TEMPLATE_BUTTON_SET_DESC">
			<option value="default">COM_KUNENA_TEMPLATE_DEFAULT</option>
			<option value="bluebird">COM_KUNENA_TEMPLATE_BUTTON_SET_BLUEBIRD</option>
			<option value="graybird">COM_KUNENA_TEMPLATE_BUTTON_SET_GRAYBIRD</option>
			<option value="blackbird">COM_KUNENA_TEMPLATE_BUTTON_SET_BLACKBIRD</option>
			<option value="whitebird">COM_KUNENA_TEMPLATE_BUTTON_SET_WHITEBIRD</option>
		</param>
		<param name="profileIconset" type="list" default="default" label="COM_KUNENA_TEMPLATE_PROFILE_ICON_SET" description="COM_KUNENA_TEMPLATE_PROFILE_ICON_SET_DESC">
			<option value="default">COM_KUNENA_TEMPLATE_DEFAULT</option>
		</param>
		<param name="editorIconset" type="list" default="default" label="COM_KUNENA_TEMPLATE_EDITOR_ICON_SET" description="COM_KUNENA_TEMPLATE_EDITOR_ICON_SET_DESC">
			<option value="default">COM_KUNENA_TEMPLATE_DEFAULT</option>
		</param>

		<param name="forumHeadercolor" type="text" default="#060C17" label="COM_KUNENA_JF_HEADER_BACKGROUND" description="COM_KUNENA_JF_TYPE_FORMAT" />
		<param name="forumHeaderBordercolor" type="text" default="#B00000" label="COM_KUNENA_JF_HEADER_BORDER_BACKGROUND" description="COM_KUNENA_JF_TYPE_FORMAT" />
		<param name="forumLinkcolor" type="text" default="#C00000" label="COM_KUNENA_JF_LINK_COLOR" description="COM_KUNENA_JF_TYPE_FORMAT" />
		<param name="forumLinkhover" type="text" default="#FF0000" label="COM_KUNENA_JF_LINK_HOVER_COLOR" description="COM_KUNENA_JF_TYPE_FORMAT" />
		<param name="forumJFButtoncolor" type="text" default="#CC0000" label="COM_KUNENA_JF_BUTTON_COLOR" description="COM_KUNENA_JF_TYPE_FORMAT" />
		<param name="forumJFButtonhover" type="text" default="#B00000" label="COM_KUNENA_JF_BUTTON_HOVER_COLOR" description="COM_KUNENA_JF_TYPE_FORMAT" />
		<param name="forumCOREButtoncolor" type="text" default="#B00000" label="COM_KUNENA_JF_CORE_BUTTON_COLOR" description="COM_KUNENA_JF_TYPE_FORMAT" />
		<param name="forumCOREButtonhover" type="text" default="#2E333C" label="COM_KUNENA_JF_CORE_BUTTON_HOVER_COLOR" description="COM_KUNENA_JF_TYPE_FORMAT" />
		
		<param name="jf-enablejQuery" type="list" default="0" label="Enable jQuery" description="Enable jQuery Liabrary if you dont have on your website, On 'JF Corporate' you dont need to enable it">
			<option value="0">COM_KUNENA_A_NO</option>
			<option value="1">COM_KUNENA_A_YES</option>
		</param>
		<param name="jf-enablejQuery-noConflict" type="list" default="0" label="Enable jQuery noConflict Mode" description="Enables jQuery noConflict Mode">
			<option value="0">COM_KUNENA_A_NO</option>
			<option value="1">COM_KUNENA_A_YES</option>
		</param>
		<param name="jf-enablefont" type="list" default="0" label="Enable Font" description="Enable Google Font 'Yanone Kaffeesatz' if you dont have on your website, On 'JF Corporate' you dont need to enable it">
			<option value="0">COM_KUNENA_A_NO</option>
			<option value="1">COM_KUNENA_A_YES</option>
		</param>
		<param name="jf-enablePiroBox" type="list" default="0" label="Enable PiroBox" description="Enable Pirobox Gallery (We need it for 'JF Kunena Login' and 'JF kunena Search' module positions) if you dont have on your website, On 'JF Corporate' you dont need to enable it">
			<option value="0">COM_KUNENA_A_NO</option>
			<option value="1">COM_KUNENA_A_YES</option>
		</param>

		<param name="enableSkinner" type="list" default="0" label="COM_KUNENA_TEMPLATE_SKINNER" description="COM_KUNENA_TEMPLATE_SKINNER_DESC">
			<option value="0">COM_KUNENA_NO</option>
			<option value="1">COM_KUNENA_YES</option>
		</param>

		<param name="templatebyText" type="hidden" default="" />
		<param name="templatebyName" type="hidden" default="" />
		<param name="templatebyLink" type="hidden" default="" />
		
		<!-- Required avatar sizes -->
		<param name="avatarSizeXThumb" type="hidden" default="36" />
		<param name="avatarSizeYThumb" type="hidden" default="36" />
		<param name="avatarSizeXWelcome" type="hidden" default="72" />
		<param name="avatarSizeYWelcome" type="hidden" default="72" />
		<param name="avatarSizeXList" type="hidden" default="36" />
		<param name="avatarSizeYList" type="hidden" default="36" />
		<param name="avatarSizeXPost" type="hidden" default="144" />
		<param name="avatarSizeYPost" type="hidden" default="144" />
		<param name="avatarSizeXProfile" type="hidden" default="200" />
		<param name="avatarSizeYProfile" type="hidden" default="200" />
	</params>
</kinstall>
